{
  "name": "@gmx-io/sdk",
  "version": "0.2.1",
  "main": "build/src/index.js",
  "exports": {
    ".": {
      "import": "./build/src/index.js",
      "require": "./build/src/index.js"
    },
    "./abis/*": "./build/abis/*",
    "./prebuilt/*": "./build/prebuilt/*",
    "./utils/*": "./build/src/utils/*",
    "./types/*": "./build/src/types/*",
    "./configs/*": "./build/src/configs/*"
  },
  "scripts": {
    "prebuild": "npx tsx scripts/prebuild",
    "prepare": "ts-patch install -s",
    "copy:jsons": "cp -r src/abis build/src && cp -r src/prebuilt build/src",
    "test": "vitest",
    "test:ci": "vitest run",
    "tscheck": "tsc -p tsconfig.json --noEmit",
    "build": "rm -rf build && tsc -p tsconfig.json && npm run copy:jsons",
    "postinstall": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ethers": "6.12.1",
    "cross-fetch": "4.0.0",
    "graphql": "15.8.0",
    "isomorphic-performance": "5.1.1",
    "lodash": "4.17.21",
    "query-string": "7.1.1",
    "typescript": "5.4.2",
    "universal-perf-hooks": "1.0.1",
    "viem": "^2.7.20"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@types/lodash": "4.14.198",
    "@types/node": "18.7.13",
    "lint-staged": "12.3.4",
    "ts-patch": "3.2.1",
    "tsx": "4.19.0",
    "typescript-transform-paths": "3.5.1",
    "vitest": "3.0.4"
  },
  "files": [
    "build"
  ]
}
